.musicWrapper{
  height: 200px;
  width: 100%;
  display: block;
  margin-right: auto;
  margin-left: auto;
  margin-top: 40px;
  margin-bottom: 25px;
}



.h2cir {
color: #555;
  font:14px 'Montserrat', sans-serif;
  left: 0;
  margin: 35px 0 0 0;
  top: 0px;
  text-align: center;
position: relative;
font-size: 12px;
text-transform:uppercase;
}
      
.circle{
    z-index: 2;
  width: 160px;
  height: 100px;
  border: 20px solid #555;
  border-top-left-radius: 7px;
  border-top-right-radius: 7px;
  border-bottom-left-radius: 50px;  
  border-bottom-right-radius: 50px;
  border-top: 0px; 
  position: relative;
  top: 40px;
  display: block;
  margin-left: auto;
  margin-right: auto;
  overflow: hidden;
}



#bars {
  height: 100px;
position: relative;
width: 120px;
display: block;
margin-left: auto;
margin-right: auto;
right:1px ;
}
      

  .bar {
    background: #666;
    bottom: 1px;
    height: 50px;
    position: absolute;
  width: 20px;
border-top-left-radius: 25px;
border-top-right-radius: 25px;
    
z-index: 1;
}

.bar:first-child{ 
border-bottom-left-radius: 25px; 
}

.bar:last-child{
border-bottom-right-radius: 25px;
}


