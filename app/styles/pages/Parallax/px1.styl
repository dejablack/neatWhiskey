// .aboutTextAnimationOne
// 	animation: rotateInDownLeft 2s
	#aboutText 
		@extend .sf1;
		color: $c2;
		font-size: 200px;
		text-align: center;
		margin: 90px 0 0 0px;
		position: absolute;
		top: 0%;
		width: 100%;
		line-height: 80px;
		font-variant: small-caps; 
		//opacity: .85;
		+below(2)
			display: none;

	#parallaxBGPX1
		background-size: 100%;
		background-position:0 100% ;
		+below(2)
			display: none;
//overLay	
	#parallaxPX1 #greenOverlayPX1
		height: 300px;
		background-image: url("../images/px1/greenOverlay.png");
		background-size: cover;
		background-position: 0px 0px;
		margin-top: -300px;
		transform:translateX(-100%); 
	#parallaxPX1 #blueOverlayPX1
		height: 300px;
		background-image: url("../images/px1/blueOverlay.png");
		background-size: cover;
		background-position: 0px 0px;
		margin-top: -50px; 
		transform:translateX(100%);

	#parallaxPX1 #parallaxBGPX1
		position: relative;
		height: 300px;
		background-image: url("../images/px1/px1Pic.jpg");
		background-size: cover;
		+below(2)
			display: none;


//Shadow
	#parallaxBGPX1 .top
		height: 10px;
		width: 100%;
		position: absolute;
		top: 0;
		background-image: url("../images/parallax_shadow_top.png");
		+below(2)
			display: none;		

	#parallaxBGPX1 .bottom 
		height: 10px;
		width: 100%;
		position: absolute;
		bottom: 0;
		background-image: url("../images/parallax_shadow_bottom.png");
		+below(2)
			display: none;

	////-------------------*\
	//// Mobile Styles
	////-------------------*/

	.mobilePX1Shadow
		display: none;
		+below(2)
			display: block;
			background: $g2;
		.top
			height: 10px;
			width: 100%;
			top: 0;
			background-image: url("../images/parallax_shadow_top.png");
		.bottom
			height: 10px;
			width: 100%;
			bottom: 0;
			background-image: url("../images/parallax_shadow_bottom.png");

	.mobilePX1
		+below(2)
			background: $g2;
			min-height: 110px;

			h1
				@extend .reset;
				@extend .sf1;
				padding-top: 30px;
				padding-bottom: 40px;
				text-shadow: $textShadow;
				text-align: center;
				color: $c2;
				letter-spacing: 18px;



